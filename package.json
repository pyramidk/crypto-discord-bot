	  "react": "^18.1.0",
	  "react-dom": "^18.1.0",
	  "reflect-metadata": "^0.1.13",
	  "rimraf": "^3.0.2",
	  "rxjs": "^7.2.0",
	  "better-sqlite3": "^7.5.1",
	  "binance-api-node": "^0.11.36",
	  "ccxt": "^1.80.75",
	  "commander": "^9.2.0",
	  "crypto": "^1.0.1",
	  "discord.js": "^13.6.0",
	  "lodash": "^4.17.21",
	  "moment": "^2.29.3",
	  "percent": "^2.2.0",
	  "queue-promise": "^2.2.1",
